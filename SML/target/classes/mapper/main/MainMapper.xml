<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sml.main.MainMapper">

    <select id="selectMain" resultType="MainVO">
        SELECT I_NUM, TITLE FROM COIN
        WHERE I_NUM = 2
    </select>

    <insert id="insertMain" parameterType="LinkedHashMap">
        INSERT INTO COIN
        (I_NUM, TITLE)
        VALUES
        (#{I_NUM}, #{TITLE})
    </insert>

    <insert id="insertMain2" parameterType="LinkedHashMap">
        INSERT INTO COIN
        (I_NUM, TITLE)
        VALUES
        (1, #{TITLE})
    </insert>

</mapper>